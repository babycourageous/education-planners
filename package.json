{
  "name": "education-planners",
  "version": "2.5.0",
  "description": "Website for Education Planners.",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all clean build:assets build:jekyll:dev --parallel serve watch:*",
    "clean": "rm -rf _site",
    "build": "run-s build:assets build:purgecss build:jekyll:prod",
    "prebuild": "npm run clean",
    "build:assets": "mkdir _site && run-p build:assets:*",
    "build:assets:css": "postcss _assets/css/index.css -o _site/assets/styles.css",
    "build:assets:js": "webpack _assets/javascript/main.js -o _site/assets/bundle.js",
    "build:assets:images": "node imagemin.images.js",
    "build:assets:svg": "svg-sprite --svg-xmldecl=false --svg-doctype=false --symbol --symbol-inline=true --symbol-dest=. --dest=_includes _assets/svg/*.svg",
    "build:assets:pdf": "mkdir -p _site/assets/pdf && cp -R _assets/pdf/* _site/assets/pdf",
    "build:assets:favicons": "cp _assets/favicons/* _site",
    "build:purgecss": "purgecss -c purgecss.config.js -o _site/assets",
    "build:jekyll:dev": "jekyll build --config='_config.yml,_config.dev.yml'",
    "build:jekyll:prod": "jekyll build",
    "watch:css": "chokidar '_assets/css/**/*' 'tailwind.js' --command 'run-s build:assets:css browsersync:reload'",
    "watch:js": "chokidar '_assets/scripts/*' --command 'run-s build:assets:js browsersync:reload'",
    "watch:images": "chokidar '_assets/images/**/*' --command 'run-s build:assets:images browsersync:reload'",
    "watch:svg": "chokidar '_assets/svg/**/*.svg' --command 'run-s build:svg build:jekyll:dev browsersync:reload'",
    "watch:jekyll": "chokidar '*.html' '*.md' '_includes/*' '_layouts/*' '_data/*.+(yml|yaml|csv|json)' '_config.yml' '_posts/*' --command 'run-s build:jekyll:dev browsersync:reload'",
    "serve": "browser-sync '_site' --port 4000 --no-notify",
    "browsersync:reload": "browser-sync reload --port 4000",
    "browsersync:stream": "browser-sync reload --port 4000 --files='assets/styles.css'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://babycourageous@github.com/babycourageous/education-planners.git"
  },
  "author": "Rene Dellefont",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/babycourageous/education-planners/issues"
  },
  "homepage": "https://github.com/babycourageous/education-planners#readme",
  "dependencies": {
    "a11y-dialog-component": "^5.1.0",
    "flickity": "^2.2.0",
    "gumshoejs": "github:cferdinandi/gumshoe",
    "smooth-scroll": "^15.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.8",
    "browser-sync": "^2.26.3",
    "chokidar": "^2.1.2",
    "chokidar-cli": "^1.2.2",
    "cssnano": "^4.1.10",
    "imagemin": "^6.1.0",
    "imagemin-cli": "^4.0.0",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-pngquant": "^7.0.0",
    "imagemin-svgo": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.2",
    "postcss-import": "^12.0.1",
    "precss": "^4.0.0",
    "purgecss": "^1.1.0",
    "svg-sprite": "^1.5.0",
    "tailwindcss": "^0.7.4",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ]
}
